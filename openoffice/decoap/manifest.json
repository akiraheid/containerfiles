{
	"appName": "OpenOffice",
	"containerName": "openoffice",
	"detach": true,
	"env": ["DISPLAY=unix${DISPLAY}"],
	"rm": true,
	"volumes": [
		"$HOME/Documents/openoffice/:/root/Documents/:rw",
		"$HOME/.config/openoffice/:/root/.openoffice/:rw",
		"/tmp/.X11-unix:/tmp/.X11-unix",
		"/usr/share/X11/xkb/:/usr/share/X11/xkb/:ro"
	]
}
