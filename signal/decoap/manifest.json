{
	"appName": "GnuCash",
	"containerName": "gnucash",
	"detach": true,
	"env": [
		"DISPLAY=unix${DISPLAY}",
		"GNC_DATA_HOME=/root/.local/share/gnucash",
		"GNC_CONFIG_HOME=/root/.config/gnucash"
	],
	"mimeTypes": [],
	"rm": true,
	"volumes": [
		"$HOME/Documents/gnucash/data:/root/gnucash",
		"$HOME/Documents/gnucash/config:/root/.config/gnucash",
		"$HOME/Documents/gnucash/share:/root/.local/share/gnucash",
		"/tmp/.X11-unix:/tmp/.X11-unix",
		"/usr/share/fonts/:/usr/share/fonts:ro"
	]
}
